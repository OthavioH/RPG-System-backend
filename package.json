    "fastify": "^4.17.0",
    "mysql2": "^2.3.3",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.1",
    "typeorm": "^0.3.7",
    "typescript": "^4.5.5",
    "uuid": "^11.1.0"
  },
