  "node": "^18",
  "scripts": {
    "start": "node dist/src/app.js",
    "start:prod": "npm run build && npm run start",
    "dev": "nodemon --watch src --ext ts,json --exec ts-node src/app.ts",
    "dev:db": "docker-compose -f ./docker-compose.db.yml up -d",
